{
  "$fuchsia/autocorrelator": {
    "ci_bucket": "global.ci"
  },
  "$fuchsia/rbe": {
    "instance": "projects/rbe-fuchsia-prod/instances/default"
  },
  "$fuchsia/resultstore": {
    "cas_instance": "projects/rbe-fuchsia-prod/instances/default",
    "project_id": "fuchsia",
    "results_url": "http://go/fxbtx",
    "rs_instance": "projects/rbe-fuchsia-prod/instances/default"
  },
  "no_python2": true,
  "recipe": "fuchsia/build",
  "spec": {
    "artifact_gcs_bucket": "fuchsia-artifacts",
    "build": {
      "enable_sandboxing": true,
      "fint_params_path": "integration/infra/config/generated/fuchsia/fint_params/try/clang_toolchain.ci.core.x64-debug-tot-build_only.textproto",
      "report_binary_sizes": true,
      "timeout_secs": 10800
    },
    "checkout": {
      "manifest": "flower",
      "project": "integration",
      "remote": "https://fuchsia.googlesource.com/integration"
    }
  }
}
